{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "sourceMap": false,
    "outDir": "dist",
    "baseUrl": ".",
    "incremental": true,
    "resolveJsonModule": true,
    "types": ["node", "express"],
    "allowJs": true
  },
  "exclude": ["node_modules", "dist"]
}
